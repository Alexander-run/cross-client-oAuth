{
    "manifest_version": 3,
    "name": "oAuth client",
    "version": "1.0",
    "description": "A Chrome extension that opens URLs received from a WebSocket server in new tabs",
    "permissions": [
      "activeTab",
      "tabs",
      "storage",
      "webRequest"
    ],
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "48": "icon.png"
    }
}